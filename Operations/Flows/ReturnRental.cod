class "Возврат из аренды" from Data.RentalDoc;

inobject public

  field  Album       :refAlbums from Album
  -- field Sum          :Numeric from Sum

  oper ReturnRental
    transaction MusicFlow.Вналичии, MusicFlow.ВАренде, Album = Album, Sum = Album?.Price^refUnit.руб, Comment = 'Возврат из аренды'
    -- transaction MusicFlow.ДС, MusicFlow.ФинРез, Album = Album, Sum = Sum^refUnit.руб, Comment = 'Доход от аренды'
  end


  oper PlanReturnRental
    transaction MusicFlow.Вналичии, MusicFlow.ВАренде, Album = Album, Sum = Album?.Price^refUnit.руб, IsPlan = true, Comment = 'Возврат из аренды'
  end

end

